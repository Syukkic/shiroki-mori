{
  "name": "Shiroki Mori",
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "colors": {
    "editor.background": "#ececec",
    "editor.foreground": "#000000",
    "editorLineNumber.foreground": "#b2b2b2",
    "editorLineNumber.activeForeground": "#000000",
    "editor.selectionBackground": "#005e8752",
    "editor.selectionForeground": "#ffffff",
    "editor.selectionHighlightBackground": "#e1e1e1",
    "editor.wordHighlightBackground": "#e4e4e4",
    "editor.findMatchBackground": "#d78700",
    "editor.findMatchHighlightBackground": "#ffd7af",
    "editor.hoverHighlightBackground": "#eeeeee",
    "editor.lineHighlightBackground": "#e1e1e1",
    "editorLink.activeForeground": "#005faf",
    "editorIndentGuide.background1": "#b2b2b2",
    "editorIndentGuide.activeBackground1": "#5f5f5f",
    "editorRuler.foreground": "#b2b2b2",
    "editorCursor.foreground": "#000000",
    "editorWhitespace.foreground": "#909090",
    "editorBracketMatch.background": "#ffffff",
    "editorBracketMatch.border": "#af5f00",
    "editorInlayHint.background": "#c4c4c41a",
    "editorInlayHint.foreground": "#969696",
    "editorInlayHint.parameterBackground": "#c4c4c41a",
    "editorInlayHint.parameterForeground": "#969696",
    "editorInlayHint.typeBackground": "#c4c4c41a",
    "editorInlayHint.typeForeground": "#969696",
    "activityBar.background": "#e4e4e4",
    "activityBar.foreground": "#000000",
    "activityBar.inactiveForeground": "#8a8a8a",
    "activityBarBadge.background": "#717171c4",
    "activityBarBadge.foreground": "#ffffff",
    "sideBar.background": "#e1e1e1",
    "sideBar.foreground": "#000000",
    "sideBar.border": "#d0d0d0",
    "sideBarTitle.foreground": "#000000",
    "sideBarSectionHeader.background": "#e4e4e4",
    "sideBarSectionHeader.foreground": "#000000",
    "list.activeSelectionBackground": "#b2b2b2",
    "list.activeSelectionForeground": "#000000",
    "list.inactiveSelectionBackground": "#d0d0d0",
    "list.hoverBackground": "#e4e4e4",
    "list.highlightForeground": "#005faf",
    "titleBar.activeBackground": "#e4e4e4",
    "titleBar.activeForeground": "#000000",
    "titleBar.inactiveBackground": "#e1e1e1",
    "titleBar.inactiveForeground": "#5f5f5f",
    "titleBar.border": "#d0d0d0",
    "statusBar.background": "#ececec",
    "statusBar.foreground": "#000000",
    "statusBar.noFolderBackground": "#ececec",
    "statusBar.debuggingBackground": "#af0000",
    "statusBar.debuggingForeground": "#ffffff",
    "tab.activeBackground": "#ececec",
    "tab.activeForeground": "#000000",
    "tab.inactiveBackground": "#e1e1e1",
    "tab.inactiveForeground": "#5f5f5f",
    "tab.border": "#d0d0d0",
    "panel.background": "#e4e4e4",
    "panel.border": "#d0d0d0",
    "panelTitle.activeForeground": "#000000",
    "panelTitle.inactiveForeground": "#5f5f5f",
    "terminal.background": "#ececec",
    "terminal.foreground": "#000000",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#d70000",
    "terminal.ansiGreen": "#008700",
    "terminal.ansiYellow": "#af5f00",
    "terminal.ansiBlue": "#005faf",
    "terminal.ansiMagenta": "#870087",
    "terminal.ansiCyan": "#008787",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBrightBlack": "#8a8a8a",
    "terminal.ansiBrightRed": "#ff0000",
    "terminal.ansiBrightGreen": "#5faf5f",
    "terminal.ansiBrightYellow": "#d78700",
    "terminal.ansiBrightBlue": "#005f87",
    "terminal.ansiBrightMagenta": "#5f00d7",
    "terminal.ansiBrightCyan": "#458588",
    "terminal.ansiBrightWhite": "#ffffff",
    "gitDecoration.addedResourceForeground": "#008700",
    "gitDecoration.modifiedResourceForeground": "#af5f00",
    "gitDecoration.deletedResourceForeground": "#af0000",
    "gitDecoration.untrackedResourceForeground": "#005faf",
    "gitDecoration.ignoredResourceForeground": "#8a8a8a",
    "gitDecoration.conflictingResourceForeground": "#ff0000",
    "diffEditor.insertedTextBackground": "#afd7af",
    "diffEditor.removedTextBackground": "#ffd7d7",
    "diffEditor.insertedLineBackground": "#afd7af66",
    "diffEditor.removedLineBackground": "#ffd7d766",
    "editorGutter.addedBackground": "#008700",
    "editorGutter.modifiedBackground": "#af5f00",
    "editorGutter.deletedBackground": "#af0000",
    "input.background": "#ffffff",
    "input.foreground": "#000000",
    "input.border": "#d0d0d0",
    "inputOption.activeBorder": "#005faf",
    "input.placeholderForeground": "#8a8a8a",
    "dropdown.background": "#ffffff",
    "dropdown.foreground": "#000000",
    "dropdown.border": "#d0d0d0",
    "button.background": "#008700",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#005f00",
    "button.secondaryBackground": "#5f5f5f",
    "button.secondaryForeground": "#ffffff",
    "checkbox.background": "#ffffff",
    "checkbox.foreground": "#000000",
    "checkbox.border": "#d0d0d0",
    "scrollbar.shadow": "#00000026",
    "scrollbarSlider.background": "#5f5f5f66",
    "scrollbarSlider.hoverBackground": "#5f5f5f99",
    "scrollbarSlider.activeBackground": "#5f5f5fb3",
    "badge.background": "#008700",
    "badge.foreground": "#ffffff",
    "progressBar.background": "#008700",
    "menu.background": "#ffffff",
    "menu.foreground": "#000000",
    "menu.selectionBackground": "#b2b2b2",
    "menu.selectionForeground": "#000000",
    "menu.separatorBackground": "#d0d0d0",
    "menubar.selectionBackground": "#b2b2b2",
    "menubar.selectionForeground": "#000000",
    "notification.background": "#e4e4e4",
    "notification.foreground": "#000000",
    "notification.buttonBackground": "#008700",
    "notification.buttonForeground": "#ffffff",
    "notification.errorBackground": "#ffd7d7",
    "notification.errorForeground": "#af0000",
    "peekView.border": "#005faf",
    "peekViewEditor.background": "#e4e4e4",
    "peekViewEditor.matchHighlightBackground": "#ffffff",
    "peekViewResult.background": "#e4e4e4",
    "peekViewResult.matchHighlightBackground": "#ffffff",
    "peekViewTitle.background": "#e4e4e4",
    "breadcrumb.background": "#ececec",
    "breadcrumb.foreground": "#5f5f5f",
    "breadcrumb.focusForeground": "#000000",
    "settings.headerForeground": "#000000",
    "settings.modifiedItemIndicator": "#008700",
    "welcomePage.background": "#ececec",
    "welcomePage.buttonBackground": "#008700",
    "welcomePage.buttonHoverBackground": "#005f00",
    "widget.shadow": "#00000026",
    "editorWidget.background": "#e4e4e4",
    "editorWidget.border": "#d0d0d0",
    "editorHoverWidget.background": "#e4e4e4",
    "editorHoverWidget.border": "#d0d0d0",
    "editorSuggestWidget.background": "#e4e4e4",
    "editorSuggestWidget.border": "#d0d0d0",
    "editorSuggestWidget.highlightForeground": "#005faf",
    "editorSuggestWidget.selectedBackground": "#b2b2b2",
    "debugToolBar.background": "#e4e4e4",
    "debugIcon.breakpointForeground": "#ff0000",
    "debugIcon.breakpointDisabledForeground": "#8a8a8a",
    "focusBorder": "#005faf",
    "foreground": "#000000",
    "selection.background": "#005e8752",
    "descriptionForeground": "#5f5f5f",
    "errorForeground": "#af0000"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8a8a8a"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Colors",
      "scope": ["colors"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#af0000"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#005faf"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#005f87"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#008787"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#870087"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.class variable.object.property"],
      "settings": {
        "foreground": "#af0000"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#af0000"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#af5f00"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#008700"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#5f00d7"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#008787"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#8a8a8a"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#af0000"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#af0000"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "foreground": "#870087"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#870087"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#5f00d7"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#af5f00"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#af5f00"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#870087"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#008700"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#af0000"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#5f00d7"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#008787"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#008787"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#870087"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#af0000"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#5f00d7"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#870087"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#008787"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#af0000"
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#008700"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#005f87"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#870087"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#af0000"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#af0000"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#af0000"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#008700"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#005f87"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#008787"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.markdown"],
      "settings": {
        "foreground": "#870087"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#af5f00"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#5f00d7"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": ["markup.raw.block.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block",
      "scope": ["punctuation.definition.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "variable.language.fenced.markdown",
        "punctuation.section.class.end"
      ],
      "settings": {
        "foreground": "#5f5f5f"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#5f5f5f"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#005f87"
      }
    },
    {
      "name": "Markup - Table",
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#000000"
      }
    }
  ]
}
